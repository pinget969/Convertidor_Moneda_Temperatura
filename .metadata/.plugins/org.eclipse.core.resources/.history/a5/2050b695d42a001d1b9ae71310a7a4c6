package ConversorMoneda;

import javax.swing.JOptionPane;

public class Moneda {
	String tipo; //PESO
	Double monto; // 300
	
	public Moneda(){
		
	}
	
		public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public Double getMonto() {
		return monto;
	}
	public void setMonto(Double moneda) {
		this.monto = moneda;
	}
		
		
		
		//double PesoADolar = 300;
		//double MonedaPeso = 0.0033;
		
		
}

